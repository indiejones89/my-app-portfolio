<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ButtonFont" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffe250</item>
        <item name="android:typeface">monospace</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="HeadingFont" parent="@android:style/TextAppearance.Large">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ff57ff79</item>
        <item name="android:typeface">sans</item>


    </style>


</resources>
